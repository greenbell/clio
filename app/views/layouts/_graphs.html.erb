<% if graphs %>
  <ul class="nav nav-tabs">
  <% graphs.each do |graph| %>
    <li><%= link_to graph.name, "#{graph.id}", :"data-toggle" => "tab" %></li>
  <% end %>
  </ul>
  <div class="tab-content">
  <% graphs.each_with_index do |graph, i| %>
    <%= content_tag :div, :class => "tab-pane#{" active" if i == 0}", :id => graph.id do %>
      <%= image_tag graph.dataurl %>
    <% end %>
  <% end %>
  </div>
<% end %>
